# MISP repositories
[mariadb]
name=mariadb
baseurl=http://yum.mariadb.org/10.1/centos7-amd64/
enabled=1
gpgkey=https://yum.mariadb.org/RPM-GPG-KEY-MariaDB
gpgcheck=1

[misp]
name=misp
baseurl=https://certrepo.switch.ch/certrepo/misp
enabled=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-KOJI-SWITCH
gpgcheck=1
